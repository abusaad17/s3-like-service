    "@azure/opentelemetry-instrumentation-azure-sdk": "^1.0.0-beta.5",
    "@azure/service-bus": "^7.9.4",
    "@azure/web-pubsub": "^1.1.1",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
