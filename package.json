    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "moment": "^2.30.1",
    "mongoose": "^8.3.1",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
